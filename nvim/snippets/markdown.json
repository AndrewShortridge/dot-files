  "Dataview Table": {
    "prefix": "dvtable",
    "body": [
      "```dataview",
      "TABLE ${1:field1}, ${2:field2}",
      "FROM ${3:\"folder\"}",
      "WHERE ${4:condition}",
      "SORT ${5:field} ${6|ASC,DESC|}",
      "```",
      "$0"
    ],
    "description": "Dataview TABLE query scaffold"
  },
  "Dataview List": {
    "prefix": "dvlist",
    "body": [
      "```dataview",
      "LIST",
      "FROM ${1:\"folder\"}",
      "WHERE ${2:condition}",
      "SORT ${3:field} ${4|ASC,DESC|}",
      "```",
      "$0"
    ],
    "description": "Dataview LIST query scaffold"
  },
  "Dataview Task": {
    "prefix": "dvtask",
    "body": [
      "```dataview",
      "TASK",
      "FROM ${1:\"folder\"}",
      "WHERE ${2:!completed}",
      "SORT ${3:due} ${4|ASC,DESC|}",
      "```",
      "$0"
    ],
    "description": "Dataview TASK query scaffold"
  },
  "Dataview JS": {
    "prefix": "dvjs",
    "body": [
      "```dataviewjs",
      "const pages = dv.pages('${1:\"folder\"}')",
      "  .where(p => ${2:p.type === \"note\"})",
      "  .sort(p => ${3:p.date}, '${4:desc}');",
      "",
      "dv.table(",
      "  [${5:\"Name\", \"Date\"}],",
      "  pages.map(p => [p.file.link, ${6:p.date}])",
      ");",
      "```",
      "$0"
    ],
    "description": "Dataviewjs block with dv.table() scaffold"
  },
  "Vault Lua Block": {
    "prefix": "vault",
    "body": [
      "```vault",
      "dv.table({${1:\"Name\", \"Status\"}}, dv.pages('${2:\"folder\"}')",
      "  :where(function(p) return ${3:p.status ~= \"Complete\"} end)",
      "  :sort(\"${4:file.name}\")",
      "  :map(function(p) return {p.file.link, ${5:p.status}} end))",
      "```",
      "$0"
    ],
    "description": "Vault (Lua) block with dv.* PageArray chaining"
  },
  "Frontmatter Basic": {
    "prefix": "fm",
    "body": [
      "---",
      "type: ${1:note}",
      "date: ${2:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}",
      "tags:",
      "  - ${3:tag}",
      "---",
      "",
      "$0"
    ],
    "description": "Basic YAML frontmatter skeleton"
  },
  "Frontmatter Task": {
    "prefix": "fmtask",
    "body": [
      "---",
      "type: task",
      "status: ${1|todo,in-progress,done,blocked|}",
      "priority: ${2|1,2,3,4,5|}",
      "due: ${3:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}",
      "project: ${4:project-name}",
      "tags:",
      "  - ${5:tag}",
      "---",
      "",
      "$0"
    ],
    "description": "Task note frontmatter with status, priority, due, project"
  },
  "Frontmatter Literature": {
    "prefix": "fmlit",
    "body": [
      "---",
      "type: literature",
      "authors:",
      "  - ${1:Author Name}",
      "year: ${2:$CURRENT_YEAR}",
      "journal: ${3:Journal Name}",
      "doi: ${4:10.xxxx/xxxxx}",
      "tags:",
      "  - ${5:literature}",
      "---",
      "",
      "$0"
    ],
    "description": "Literature note frontmatter with authors, year, journal, doi"
  },
  "Task Item": {
    "prefix": "task",
    "body": [
      "- [ ] ${1:description} [due:: ${2:date}] [priority:: ${3:1}]"
    ],
    "description": "Inline task with due date and priority"
  },
  "Task Done": {
    "prefix": "taskdone",
    "body": [
      "- [x] ${1:description}"
    ],
    "description": "Completed task checkbox"
  },
  "Wiki Link": {
    "prefix": "wl",
    "body": [
      "[[${1:note name}]]$0"
    ],
    "description": "Obsidian wiki link"
  },
  "Wiki Link Aliased": {
    "prefix": "wla",
    "body": [
      "[[${1:note name}|${2:display text}]]$0"
    ],
    "description": "Obsidian wiki link with display text alias"
  },
  "Embed": {
    "prefix": "embed",
    "body": [
      "![[${1:note name}]]$0"
    ],
    "description": "Obsidian embed (transclusion)"
  },
  "Heading 2": {
    "prefix": "h2",
    "body": [
      "## ${1:Heading}",
      "",
      "$0"
    ],
    "description": "Level 2 heading"
  },
  "Heading 3": {
    "prefix": "h3",
    "body": [
      "### ${1:Heading}",
      "",
      "$0"
    ],
    "description": "Level 3 heading"
  },
  "Code Block": {
    "prefix": "cb",
    "body": [
      "```${1:language}",
      "$2",
      "```",
      "$0"
    ],
    "description": "Fenced code block with language placeholder"
  },
  "Table": {
    "prefix": "table",
    "body": [
      "| ${1:Header 1} | ${2:Header 2} | ${3:Header 3} |",
      "| --- | --- | --- |",
      "| ${4:cell} | ${5:cell} | ${6:cell} |",
      "$0"
    ],
    "description": "Markdown table with header and separator"
  }
}